{
    "version": 2,
    "alias": "mars-app-gamma.vercel.app",
    "github": {
      "enabled": false
    },
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "./dist/**",
        "use": "@now/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "dist/index.html"
      }
    ]
  }